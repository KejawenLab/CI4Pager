{
    "name": "kejawenlab/ci4pager",
    "description": "CodeIgniter 4 Pagination Library",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": ">=7.2",
        "ext-pdo": "*",
        "codeigniter4/framework": ">=4.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "KejawenLab\\CodeIgniter\\Pagination\\": "src/"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "KejawenLab\\CodeIgniter\\Pagination\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev"
}
